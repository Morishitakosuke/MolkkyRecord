<div class="container-fluid py-4 mw-md">
  <ul>
    <% @users.each do |user| %>
      <li>
        <% if user.image.present? %>
          <img src=<%= user.image %> class="icon_image">
        <% else %>
          <img src="/assets/default.png" class="icon_image">
        <% end %>
        <%= link_to user.username, user %>
      </li>
    <% end %>
  </ul>
  <%= paginate @users %>
</div>
